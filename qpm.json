{
	"sharedDir": "shared",
	"dependenciesDir": "extern",
	"info": {
	  "name": "Score Viewer",
	  "id": "scoreviewer",
	  "version": "1.0.0",
	  "url": null,
	  "additionalData": {
		"overrideSoName": "libscoreviewer.so",
		"cmake": true
	  }
	},
	"workspace": {
	  "scripts": {
		"build": [
		  "pwsh ./scripts/build.ps1"
		],
		"copy": [
		  "pwsh ./scripts/copy.ps1"
		],
		"logcat": [
		  "adb logcat -c",
		  "pwsh ./scripts/start-logging.ps1"
		],
		"qmod": [
		  "pwsh ./scripts/build.ps1 -clean",
		  "qpm qmod build",
		  "pwsh ./scripts/createqmod.ps1"
		],
		"restart": [
		  "pwsh ./scripts/restart-game.ps1"
		],
		"stack": [
		  "pwsh ./scripts/ndk-stack.ps1"
		],
		"tomb": [
		  "pwsh ./scripts/pull-tombstone.ps1"
		]
	  }
	},
	"dependencies": [
	  {
		"id": "beatsaber-hook",
		"versionRange": "^3.14.0",
		"additionalData": {
		  "extraFiles": [
			"src/inline-hook"
		  ]
		}
	  },
	  {
		"id": "codegen",
		"versionRange": "^0.33.0",
		"additionalData": {}
	  },
	  {
		"id": "custom-types",
		"versionRange": "^0.15.24",
		"additionalData": {}
	  },
	  {
		"id": "rapidjson-macros",
		"versionRange": "^1.0.0",
		"additionalData": {}
	  },
	  {
		"id": "config-utils",
		"versionRange": "^1.1.0",
		"additionalData": {}
	  },
	  {
		"id": "questui",
		"versionRange": "^0.17.12",
		"additionalData": {}
	  },
	  {
		"id": "pinkcore",
		"versionRange": "^1.8.9",
		"additionalData": {}
	  },
	  {
		"id": "bsml",
		"versionRange": "^0.3.3",
		"additionalData": {}
	  },
	  {
		"id": "paper",
		"versionRange": "^1.2.9",
		"additionalData": {}
	  },
	  {
	  	"id": "song-details",
		"versionRange": "^0.3.1",
	  	"additionalData": {}
	  }
	]
